{

    "manifest_version": 2,
    "name": "uninoti",
    "description": "basic start of class notification for daan.ir",
    "version": "1.0",
    "homepage_url": "https://github.com/ohinibla/UniNoti.git",
    "icons": {
      "48": "icons/notification.png"
    },

    "permissions": [
      "notifications"
    ],

    "background": {
      "scripts": ["background-script.js"]
    },
    
    "browser_action": {
        "default_icon": "icons/notification.png",
       " default_title": "uninoti"
    },

    "content_scripts": [
      {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
      }
    ]
  }
