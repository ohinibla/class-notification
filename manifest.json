{

    "manifest_version": 2,
    "name": "uninoti",
    "description": "basic start of class notification for daan.ir",
    "version": "1.0",
    "homepage_url": "https://github.com/ohinibla/UniNoti.git",
    "icons": {
      "48": "icons/uninoti-48.png"
    },

    "permissions": [
      "activeTab",
      "notifications"
    ],

    "browser_action": {
      "default-icon": "icons/uninoti-32.png",
      "default_title": "uninoti",
      "default_popup": "popup/uninoti.html"
    },
    
    "web_accessible_resources": []

  }